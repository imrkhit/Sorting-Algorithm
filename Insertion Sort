#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[100], ele, n, i;
    printf("Enter the size of the array\n");
    scanf("%d",&n);
    printf("Enter the elements of the array\n");
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&ele);
        a[i]=ele;
    }
    insertsort(a,n);
    for(i=0;i<=n-1;i++)
        printf("%d\t",a[i]);
}

void insertsort(int a[], int n)
{
    int i, j, temp;
    for(i=1;i<n;i++)
    {
        temp=a[i];
        for(j=i-1;j>=0 && temp<a[j];j--)
            a[j+1]=a[j];
        a[j+1]=temp;
    }
    return;
}
